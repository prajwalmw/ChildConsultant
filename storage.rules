rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // This is the path your code uses: profile_pictures/{UID}/profile.jpg
    match /profile_pictures/{userId}/{fileName} {
      allow read, write: if request.auth != null && request.auth.uid == userId;
    }

    // General rule
    match /{allPaths=**} {
      allow read: if true;
    }
  }
}